# Git keywords
gcpmd   : git commit -pm "DBGSW-
gcpm    : git commit -pm "
gcpam   : git commit -p --amend
gcpamne : git commit -p --amend --no-edit
gcam    : git commit -as --amend
gcamne  : git commit -as --amend --no-edit
gchd    : git checkout -- .
gpgh    : git push github HEAD:
gpghrh  : git push github HEAD:refs/heads/
gpor    : git push origin HEAD:
gporrh  : git push origin HEAD:refs/heads/
gpy     : git push y103 HEAD:
gpyrh   : git push y103 HEAD:refs/heads/
gri     : git rebase -i 
grc     : git rebase --continue

# Source
sdi     : source ~/workspace/DI/venv/bin/activate
src     : source ~/.zshrc

# Debug Insight
din     : python -m dicore -t normal $D/
disn    : python -m dicore -it normal $D/
diin    : python -m dicore -st normal $D/
dig     : python -m dicore -t glance $D/

# Tmux
tmn     : tmux new -s  
tma     : tmux attach -t 
